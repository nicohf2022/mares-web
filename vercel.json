{
  "builds": [
    { "src": "index.html", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/sitemap.xml" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "framework": "vite",
  "outputDirectory": "dist"
}